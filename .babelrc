{
  "presets": ["react", "es2015", "stage-0"],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "panels": "./components/panels",
        "erschema": "erschema-suite/erschema",
        "erschema-actions": "erschema-suite/actions",
        "erschema-action-handlers": "erschema-suite/action-handlers",
        "erschema-selectors": "erschema-suite/selectors",
        "erschema-reducer": "erschema-suite/reducer",
        "erschema-normalize": "erschema-suite/normalize"
      }
    }],
    "transform-flow-strip-types"
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    }
  }
}
